<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="323.0" prefWidth="394.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.controller.AddSongMenuController">
   <children>
      <MenuBar VBox.vgrow="NEVER">
         <menus>
            <Menu mnemonicParsing="false" text="Add/Edit song">
               <items>
                  <MenuItem mnemonicParsing="false" text="New" />
                  <MenuItem mnemonicParsing="false" text="Open…" />
                  <Menu mnemonicParsing="false" text="Open Recent" />
                  <SeparatorMenuItem mnemonicParsing="false" />
                  <MenuItem mnemonicParsing="false" text="Close" />
                  <MenuItem mnemonicParsing="false" text="Save" />
                  <MenuItem mnemonicParsing="false" text="Save As…" />
                  <MenuItem mnemonicParsing="false" text="Revert" />
                  <SeparatorMenuItem mnemonicParsing="false" />
                  <MenuItem mnemonicParsing="false" text="Preferences…" />
                  <SeparatorMenuItem mnemonicParsing="false" />
                  <MenuItem mnemonicParsing="false" text="Quit" />
               </items>
            </Menu>
         </menus>
      </MenuBar>
      <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefWidth="394.0" VBox.vgrow="ALWAYS">
         <children>
            <Label layoutX="35.0" layoutY="40.0" text="Title" />
            <TextField fx:id="txtTitle" layoutX="68.0" layoutY="36.0" prefHeight="25.0" prefWidth="228.0" />
            <Button layoutX="255.0" layoutY="244.0" mnemonicParsing="false" onAction="#clickSave" text="Save" />
            <Button layoutX="308.0" layoutY="244.0" mnemonicParsing="false" onAction="#clickCancel" text="Cancel" />
            <Label layoutX="32.0" layoutY="77.0" text="Artist" />
            <Label layoutX="30.0" layoutY="120.0" text="Genre" />
            <Label layoutX="33.0" layoutY="161.0" text="Time" />
            <TextField fx:id="txtArtist" layoutX="68.0" layoutY="73.0" prefHeight="25.0" prefWidth="228.0" />
            <TextField fx:id="txtTime" layoutX="68.0" layoutY="157.0" prefHeight="25.0" prefWidth="51.0" />
            <TextField fx:id="txtFile" layoutX="68.0" layoutY="191.0" prefHeight="25.0" prefWidth="228.0" />
            <Label layoutX="36.0" layoutY="195.0" text="File" />
            <ChoiceBox fx:id="choiceBox" layoutX="68.0" layoutY="116.0" prefHeight="25.0" prefWidth="84.0" />
            <Button layoutX="308.0" layoutY="191.0" mnemonicParsing="false" onAction="#clickChoose" text="Choose..." />
         </children>
      </AnchorPane>
   </children>
</VBox>
