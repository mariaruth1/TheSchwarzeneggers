<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="354.0" prefWidth="394.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.controller.AddSongMenuController">
   <children>
      <MenuBar VBox.vgrow="NEVER">
         <menus>
            <Menu mnemonicParsing="false" text="Add/Edit song">
               <items>
                  <MenuItem mnemonicParsing="false" text="New" />
                  <MenuItem mnemonicParsing="false" text="Open…" />
                  <Menu mnemonicParsing="false" text="Open Recent" />
                  <SeparatorMenuItem mnemonicParsing="false" />
                  <MenuItem mnemonicParsing="false" text="Close" />
                  <MenuItem mnemonicParsing="false" text="Save" />
                  <MenuItem mnemonicParsing="false" text="Save As…" />
                  <MenuItem mnemonicParsing="false" text="Revert" />
                  <SeparatorMenuItem mnemonicParsing="false" />
                  <MenuItem mnemonicParsing="false" text="Preferences…" />
                  <SeparatorMenuItem mnemonicParsing="false" />
                  <MenuItem mnemonicParsing="false" text="Quit" />
               </items>
            </Menu>
         </menus>
      </MenuBar>
      <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="231.0" prefWidth="392.0" VBox.vgrow="ALWAYS">
         <children>
            <Label layoutX="32.0" layoutY="125.0" prefHeight="18.0" prefWidth="32.0" text="Year" textAlignment="CENTER" />
            <TextField fx:id="txtYear" layoutX="66.0" layoutY="122.0" prefHeight="25.0" prefWidth="228.0" />
            <Button layoutX="14.0" layoutY="277.0" mnemonicParsing="false" onAction="#clickSave" prefHeight="26.0" prefWidth="160.0" text="Save" />
            <Button layoutX="207.0" layoutY="277.0" mnemonicParsing="false" onAction="#clickCancel" prefHeight="26.0" prefWidth="174.0" text="Cancel" />
            <Label layoutX="30.0" layoutY="81.0" prefHeight="26.0" prefWidth="32.0" text="Artist" />
            <Label layoutX="18.0" layoutY="176.0" text="Genre" />
            <TextField fx:id="txtArtist" layoutX="66.0" layoutY="85.0" prefHeight="25.0" prefWidth="228.0" />
            <TextField fx:id="txtFile" layoutX="66.0" layoutY="14.0" prefHeight="25.0" prefWidth="228.0" />
            <Label contentDisplay="CENTER" layoutX="30.0" layoutY="17.0" prefHeight="18.0" prefWidth="32.0" text="File" />
            <ChoiceBox fx:id="choiceBox" layoutX="66.0" layoutY="172.0" prefHeight="25.0" prefWidth="84.0" />
            <Button layoutX="302.0" layoutY="14.0" mnemonicParsing="false" onAction="#clickChoose" text="Choose..." />
            <Label fx:id="text" alignment="CENTER" layoutX="24.0" layoutY="46.0" prefHeight="26.0" prefWidth="44.0" text="Title" />
            <TextField fx:id="txtTitle" layoutX="66.0" layoutY="50.0" prefHeight="26.0" prefWidth="228.0" />
            <TextField fx:id="txtErrorMessage" editable="false" layoutX="20.0" layoutY="212.0" opacity="0.0" prefHeight="55.0" prefWidth="354.0" />
         </children>
      </AnchorPane>
   </children>
</VBox>
